<div>
    <div class="panel box box-primary ">
        <div class="box-header with-border">
            <h4 class="box-title">
                Datos de la Licitacion - <span class="text-blue">{{selectedSupuesto.tablaModNombre}}</span>
            </h4>
        </div>
        <div class="box-body" style="padding:0px;">
            <div style="padding:0px;">
                <div class="box-body" ng-if="selectedSupuesto!=null">
                    <ng-tsc template="template" script="script" item="selectedSupuesto" ng-if="showSupuesto">
                    </ng-tsc>

                    <button class="btn btn-primary pull-right" style="margin: 5px;"
                            ng-click="ActualizarSupuesto()">
                        Actualizar
                    </button>
                </div>

                @Html.Partial("~/Views/Shared/_PartialLoadingPage.cshtml")
            </div>
        </div>
    </div>
</div>


